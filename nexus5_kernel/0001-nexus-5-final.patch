+int my_close(struct inode *inode, struct file *filp)
+{
+	struct mmap_info *info = filp->private_data;
+	//free_pages((unsigned long)info->data, 10);
+    	kfree(info);
+	filp->private_data = NULL;
+	return 0;
+}
