+		if (desc->action && desc->action->name) {
+			spr_log_nt("Resume caused by IRQ %d,%s %s", irq,
+				desc->name, desc->action->name);
+		} else {
+			spr_log_nt("Resume caused by IRQ %d,%s", irq,
